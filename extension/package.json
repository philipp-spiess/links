{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "links-extension",
  "title": "psp.sh links",
  "description": "List short links for psp.sh",
  "icon": "command-icon.png",
  "author": "philipp-spiess",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Links",
      "description": "Render available links",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.71.4",
    "@raycast/utils": "^1.14.0",
    "clipboardy": "^4.0.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
